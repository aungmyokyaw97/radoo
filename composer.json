{
    "name": "aungmyokyaw/radoo",
    "description": "Odoo ERP API for Laravel",
    "keywords": ["odoo","laravel","erp","package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Aung Myo Kyaw",
            "email": "aungmyokyaw28637@gmail.com"
        }
    ],
    "autoload": {
      "psr-4": {
        "Amk\\Radoo\\": "src/"
      },
      "files": [
          "src/Support/helpers.php"
      ],
      "classmap": ["src/ripcord"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amk\\Radoo\\Providers\\RadooServiceProvider"
            ],
            "aliases": {
                "Radoo" : "Amk\\Radoo\\Facades\\Radoo"
            }
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^5.6||^7.0||^8.0",
        "illuminate/support": "^7.0||^8.0||^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
